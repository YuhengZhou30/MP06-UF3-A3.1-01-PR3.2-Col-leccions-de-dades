<configuration>

    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>data/logs/PR32CreateMain.log</file>
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="DEBUG">
        <appender-ref ref="FILE" />
    </root>

</configuration>
